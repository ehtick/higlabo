		<PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="124.0.6367.20100" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\HigLabo.CodeGenerator\HigLabo.CodeGenerator.csproj" />
		<ProjectReference Include="..\HigLabo.Core\HigLabo.Core.csproj" />
		<ProjectReference Include="..\HigLabo.Mapper\HigLabo.Mapper.csproj" />
	</ItemGroup>

</Project>
