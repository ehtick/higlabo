<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>9.0.0.0</Version>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Company>HigLabo</Company>
		<Authors>Higty</Authors>
		<Copyright>Copyright Â© 2011- HigLabo All Rights Reserved</Copyright>
		<RepositoryUrl>https://github.com/higty/higlabo</RepositoryUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="AngleSharp" Version="1.1.2" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="Selenium.Support" Version="4.27.0" />
		<PackageReference Include="Selenium.WebDriver" Version="4.27.0" />
		<PackageReference Include="Selenium.WebDriver.ChromeDriver" Version="132.0.6834.8300" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\HigLabo.CodeGenerator\HigLabo.CodeGenerator.csproj" />
		<ProjectReference Include="..\HigLabo.Core\HigLabo.Core.csproj" />
		<ProjectReference Include="..\HigLabo.Mapper\HigLabo.Mapper.csproj" />
	</ItemGroup>

</Project>
